<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget" value="https://api.nuget.org/v3/index.json" />
    <add key="Telerik" value="https://nuget.telerik.com/nuget" />
  </packageSources>
  <packageSourceCredentials>
    <Telerik>
      <add key="Username" value="%nuget_telerik_user%" />      
      <add key="ClearTextPassword" value="%nuget_telerik_password%" />
    </Telerik>
  </packageSourceCredentials>
</configuration>